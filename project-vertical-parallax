
<!-- Start scrolltrigger parallax -->
<script>
    gsap.to(".hero-image", {
     scrollTrigger : {
     scrub : .45
     }, 
     y : 200
    });
</script>

<script>
    gsap.to(".line1", {
     scrollTrigger : {
     scrub : 1
     }, 
     y : 1
    });
</script>
    
<script>
    gsap.to(".line2", {
     scrollTrigger : {
     scrub : 1
     }, 
     y : 1
    });
</script>
 
<!-- End scrolltrigger parallax -->
    
